{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/experience/?$",
			"dest": "_render"
		},
		{
			"src": "^/jaczup/?$",
			"dest": "_render"
		},
		{
			"src": "^/projects/?$",
			"dest": "_render"
		},
		{
			"src": "^/secret/pl/data_leaks/?$",
			"dest": "_render"
		},
		{
			"src": "^/sources/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}